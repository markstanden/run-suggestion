{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "deploy": {
      "apiLanguage": "dotnetisolated",
      "apiVersion": "8.0",
      "appLocation": "src/Web",
      "appBuildCommand": "dotnet publish -c Release",
      "outputLocation": "src/Web/bin/Release/net8.0/publish/wwwroot",
      "apiLocation": "src/Api/bin/Release/net8.0/publish",
      "apiBuildCommand": "dotnet publish -c Release",
      "appName": "swa-runsugg-dev",
      "resourceGroup": "rg-runsugg-dev-uks",
      "open": false
    },
    "dev": {
      "apiLanguage": "dotnetisolated",
      "apiVersion": "8.0",
      "appLocation": "src/Web",
      "appBuildCommand": "dotnet publish -c Debug",
      "outputLocation": "src/Web/bin/Debug/net8.0/publish/wwwroot",
      "apiLocation": "src/Api",
      "apiBuildCommand": "dotnet publish -c Debug",
      "run": "dotnet watch run",
      "appDevserverUrl": "http://localhost:5281",
      "open": true
    },
    "test": {
      "apiLanguage": "dotnetisolated",
      "apiVersion": "8.0",
      "appLocation": "src/Web",
      "appBuildCommand": "dotnet publish -c Debug",
      "outputLocation": "src/Web/bin/Debug/net8.0/publish/wwwroot",
      "apiLocation": "src/Api",
      "apiBuildCommand": "dotnet publish -c Debug",
      "run": "dotnet run",
      "appDevserverUrl": "http://localhost:5281",
      "open": false
    }
  }
}